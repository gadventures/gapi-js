{
  "name": "gapi-js",
  "version": "4.0.2",
  "description": "A js client for GAPI",
  "main": "build/gapi-js.js",
  "module": "src/gapi-js.js",
  "scripts": {
    "start": "webpack-dev-server -d --inline --progress --colors --history-api-fallback --config webpack.development.js",
    "build": "webpack --display-error-details -p --progress --colors --config webpack.production.js",
    "build:watch": "webpack --display-error-details -p --progress --colors --watch --config webpack.production.js",
    "prepublish": "npm run build",
    "test": "jest"
  },
  "author": "Sia Malekpour",
  "license": "MIT",
  "dependencies": {
    "babel-plugin-module-resolver": "^2.7.1",
    "superagent": "2.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "clean-webpack-plugin": "^0.1.15",
    "html-webpack-plugin": "^3.0.6",
    "jest": "^19.0.2",
    "webpack": "^4.1.0",
    "webpack-cli": "^2.0.10",
    "webpack-dev-server": "^3.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:gadventures/gapi-js.git"
  },
  "homepage": "https://github.com/gadventures/gapi-js",
  "bugs": {
    "url": "https://github.com/gadventures/gapi-js/issues"
  },
  "keywords": [
    "gapi",
    "superagent"
  ]
}
